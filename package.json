{
  "name": "vue-twa-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "globals": "^15.11.0",
    "husky": "^8.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.9.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
